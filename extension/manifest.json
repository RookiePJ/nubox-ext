{
  "manifest_version": 2,
  "name": "nuBox",
  "version": "0.1",
  "author": "Robin Thomas",
  "background": {
    "scripts": ["background.js"]
  },
  "externally_connectable": {
    "matches": [
      "https://nubox.herokuapp.com/",
      "http://localhost:4000/*"
    ]
  },
  "permissions": ["nativeMessaging"]
}
